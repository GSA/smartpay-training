---
/*
This loops data array provided in home page (index.astro) and load data for individual training card.
*/
import TrainingCard from "./TrainingCard.astro";
const { items, category} = Astro.props;
---

<ul class="usa-card-group">
    {items.map(item => <TrainingCard item={item} category={category}/>)}
</ul>
    
